<script type="text/javascript">
  $headerHeight = $("header").height();
    $("body").css("padding-top", $headerHeight);
    $(".hash").css("margin-top", -$headerHeight);
    
    $(window).scroll(function() {
      $headerHeight = $("header").height();
      
      if($(window).scrollTop() > 250) {
        $("body").css("padding-top", $headerHeight);
        $(".hash").css("margin-top", -$headerHeight);
      }else{
        $("body").css("padding-top", $headerHeight);
        $(".hash").css("margin-top", -$headerHeight);
        } 
  });
</script>