.grey_box p{color: #5a5a5a; font-size: 25px; line-height: 40px;}
.how_work h3{color: #5a5a5a; font-family: 'oxfam_global_headlineregular' !important; font-size: 38px; margin-bottom: 30px; text-align: center; margin-bottom: 50px;}
.mission h3{color: #5a5a5a; font-family: 'oxfam_global_headlineregular' !important; font-size: 38px; text-align: center; margin-top: 20px; margin-bottom: 30px;}
.how_circle{background-image: linear-gradient(to top, rgba(224,2,84,1), rgba(94,37,122,1)); border-radius: 50%; padding: 30px; height: 260px; width: 260px; text-align: center;}
.how_circle h4{font-size: 80px; color: #fff; font-family: 'oxfam_global_headlineregular' !important;}
.how_circle p{font-size: 25px; text-transform: uppercase; font-family: arial; color: #fff; font-weight: bold; line-height: 30px;}
.mission p{font-size: 25px; line-height: 36px; text-align: center;}
.icon_circle{width: 114px; height: 114px; display: inline-block; color: #fff; text-align: center; line-height: 114px; border-radius: 50%; font-size: 50px;}
