.register_for span{background: #fff; padding: 0 15px; color: #74c042; font-family: 'oxfam_global_headlineregular' !important; display: block; text-align: center; font-size: 18px;}

.add_sec p{margin: 7px 0;}
.add_sec p a{color: #c3c3c3;}
.footer_logo {margin-bottom: 15px;}
.footer_right{text-align: left !important;}
