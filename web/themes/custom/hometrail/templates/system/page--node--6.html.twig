<header>
  <div class="container">
    <div class="row">
      <div class="col-md-3 col-xs-6 header_logo"><a href="{{ path('<front>') }}"><img
            src="{{ path('<front>') }}{{ directory }}/images/logo.png"></a>
      </div>
      <div class="col-md-6 col-xs-6 center stir"><h2>let’s stir things up!</h2></div>
      <div class="col-md-3 col-xs-6 goes_vtw_logo"><img src="{{ path('<front>') }}{{ directory }}/images/right_logo.png"></div>
    </div>
  </div>
</header>

<div style="display:none">
  {{ page.header }}
</div>

<div class="slider_par bg_part successful">
  <div class="container">
    <div id="carouselExampleInterval" class="carousel slide" data-ride="carousel">
      <div class="carousel-inner">
        <div class="carousel-item active">
          <img src="{{ path('<front>') }}{{ directory }}/images/big-slide.jpg" class="d-block w-100" alt="">
          <div class="caption" style="top: 24%;">
            <h1>Registration Successful</h1>
            <span class="white_part">
          <h2>You're in! Check Your Mail</h2>
          <p>Please do check your spam/ junk folders if you can’t locate the email</p>
          <p>Thank you for joining this movement of people against poverty! You can <a href="https://www.oxfamindia.org/crowd-fund" target="_blank">start a fundraiser</a> to continue supporting Oxfam India.</p>
          </span>
            <div class="clearfix"></div>
           <div class="tour_buttons">
{% if logged_in %}
        <a href="https://www.oxfamindia.org/crowd-fund" target="_blank" class="donate_btn" ><span><i class="fas fa-user"></i></span>Fundraiser</a>
         <a href="/user/logout" class="donate_btn"><span><i class="fas fa-key"></i></span> Log-out</a>
         <a href="/dashboard" class="donate_btn"><span><i class="fas fa-key"></i></span> Dashboard</a>
        {% else %}
        <a href="https://www.oxfamindia.org/crowd-fund" target="_blank" class="donate_btn" ><span><i class="fas fa-user"></i></span> Start Fundraiser</a>
         <a href="/user/register" class="donate_btn" ><span><i class="fas fa-user"></i></span> register</a>
          <a href="#donate_form" class="donate_btn" data-toggle="modal" data-target="#login-form"><span><i class="fas fa-key"></i></span> Log-in</a>
        {% endif %}

</div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>

