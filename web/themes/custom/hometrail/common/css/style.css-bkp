.secure{background: #f1f1f1; padding: 40px 0;}
.secure img{width: 100%; height: auto;}
.secure .row{padding: 15px 0;}
.modal-dialog.register{max-width: 540px; /*background-image: linear-gradient(to top, rgba(224,2,84,1), rgba(94,37,122,1));*/}
.modal-header{background: #74c042; color: #fff; font-family: 'oxfam_global_headlineregular' !important; padding: 10px 20px;}
.modal-title{font-size: 26px;}
.modal-dialog.login{max-width: 350px;}

/*for Laptop Screen*/
@media (min-width: 1024px) and (max-width: 1440px) { 
