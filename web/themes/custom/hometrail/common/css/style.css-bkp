.participate a{background: #e20153; color: #fff; padding: 5px 30px; border-radius: 20px; font-family: 'oxfam_global_headlineregular' !important; font-size: 22px; text-decoration: none;}
.participate p{font-size: 18px; margin-bottom: 20px; line-height: 30px;}
footer h2{font-family: 'oxfam_global_headlineregular' !important; font-size: 22px; color: #fff; margin-bottom: 15px;}
footer h2 span{color: #61a534;}
