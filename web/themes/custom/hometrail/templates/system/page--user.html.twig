<header>

<!-- start navigation --> 

<nav class="navbar navbar-expand-lg navbar-light bg-light">
<div class="container">
 <a class="navbar-brand" href="https://virtualtrailwalker.oxfamindia.org/"> <img src="{{ corporatelogosss }}"></a>


<button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle">
    <span class="navbar-toggler-icon"></span>
  </button>

<div class="collapse navbar-collapse" id="navbarSupportedContent">
<ul class="navbar-nav">
<li class="nav-item active"><a class="nav-link" href="{{ path('<front>') }}#home">Home</a></li>
<li class="nav-item"><a class="nav-link" href="{{ path('<front>') }}#about">About</a></li>
<li class="nav-item"><a class="nav-link " href="{{ path('<front>') }}#stories">Stories of change</a></li>
<li class="nav-item"><a class="nav-link " href="{{ path('<front>') }}#support">Support Now</a></li>
<li class="nav-item"><a class="nav-link " href="{{ path('<front>') }}#relief">Covid-19 Relief</a></li>
<li class="nav-item"><a class="nav-link " href="{{ path('<front>') }}#faq">FAQs</a></li>
</ul>
</div>

<div class="hashtag"><img src="https://oxfamuploads.s3.ap-south-1.amazonaws.com/oxfamdata/ravi/vtw-new-design/images/mnemonic-green.png"></div>

</div>
</nav>

<!-- end navigation -->
</header>
<!-- end header -->



<section class="mainfix {{ node.gettype }}">
  <div class="container" style="padding:90px 0 0px">
    {{ page.header }}
    {{ page.content }}
  </div>
</section>

{% if page.dashboard %}
  <section class="section dashboard virtual-dashboard upload_details">
    <div class="container">
      {% if page.upcoming %}
        <div class="row">
          <div class="col-md-9">
            {{ page.dashboard }}
          </div>
          <div class="col-md-3">
            {{ page.upcoming }}
          </div>
        </div>
        {% else %}
          {{ page.dashboard }}
      {% endif %}
    </div>
  </section>
{% endif %}

{% if page.leaderboard %}
  <section class="champion-leaderboard-sec">
    <div class="container">
      <h2>Champions of change – Leaderboard</h2>
    <div class="row participants-row text-center">
      <div class="col-12">
        <ul>
          <li class="active" id="leaderboardtab1"><a href="javascript:void(0)"><h3>25km <span>participants</span></h3></a></li>
          <li id="leaderboardtab2"><a href="javascript:void(0)"><h3>50km <span>participants</span></h3></a></li>
          <li id="leaderboardtab3"><a href="javascript:void(0)"><h3>100km <span>participants</span></h3></a></li>
        </ul>
      </div>
    </div>
    {{ page.leaderboard }}
    </div>
  </section>
{% endif %}

